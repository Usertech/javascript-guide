   "scripts": {
      "ab": "node ./node_modules/re-app-builder",
      "clean": "rm -rf ./public && rm -rf ./manifest",
      "dev": "yarn run clean && export $(cat .env.devserver | xargs) && NODE_ENV=development yarn run ab dev",
